   </extension>
   <extension
         point="org.eclipse.eclipsemonkey.dom">
      <dom
            class="org.eclipse.eclipsemonkey.integration.modeling.uml.DOMFactory"
            id="org.eclipse.eclipsemonkey.integration.modeling.uml.dom"
            resource="org.eclipse.eclipsemonkey.integration.modeling.uml.UmlDOM"
            variableName="uml">
      </dom>
   </extension>

