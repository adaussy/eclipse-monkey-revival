<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.eclipsemonkey.language">
      <language
            class="org.eclipse.eclipsemonkey.lang.python.PythonLanguageFactory"
            languageExtension="py"
            languageName="Python">
      </language>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.eclipse.eclipsemonkey.lang.python.PythonLibraryPreferencePage"
            id="org.eclipse.eclipsemonkey.lang.python.librairyPreferencePage"
            name="Eclipse Monkey: Python">
      </page>
   </extension>

</plugin>
